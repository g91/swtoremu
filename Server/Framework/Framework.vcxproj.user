  <PropertyGroup>
    <ShowAllFiles>true</ShowAllFiles>
    <LocalDebuggerWorkingDirectory Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'">..\Bin\$(PlatformShortName)\</LocalDebuggerWorkingDirectory>
